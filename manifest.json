{
  "manifest_version": 3,
  "name": "AutoSkipYT",
  "version": "1.1",
  "description": "Automatically skips YouTube ads by simulating clicks.",
  "author": "j4g",
  "homepage_url": "https://github.com/ScribeAegis",
  "permissions": ["tabs"],
  "host_permissions": ["*://www.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
